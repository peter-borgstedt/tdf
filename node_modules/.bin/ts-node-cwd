#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/peterborgstedt/tdf/node_modules/.pnpm/ts-node@10.4.0_@types+node@17.0.4_typescript@4.5.4/node_modules/ts-node/dist/node_modules:/Users/peterborgstedt/tdf/node_modules/.pnpm/ts-node@10.4.0_@types+node@17.0.4_typescript@4.5.4/node_modules/ts-node/node_modules:/Users/peterborgstedt/tdf/node_modules/.pnpm/ts-node@10.4.0_@types+node@17.0.4_typescript@4.5.4/node_modules:/Users/peterborgstedt/tdf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/peterborgstedt/tdf/node_modules/.pnpm/ts-node@10.4.0_@types+node@17.0.4_typescript@4.5.4/node_modules/ts-node/dist/node_modules:/Users/peterborgstedt/tdf/node_modules/.pnpm/ts-node@10.4.0_@types+node@17.0.4_typescript@4.5.4/node_modules/ts-node/node_modules:/Users/peterborgstedt/tdf/node_modules/.pnpm/ts-node@10.4.0_@types+node@17.0.4_typescript@4.5.4/node_modules:/Users/peterborgstedt/tdf/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-cwd.js" "$@"
fi
